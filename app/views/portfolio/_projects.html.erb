<li class="project yui3-u-1-3 yui3-u-m-1-2 yui3-u-s-1-2">
  <div class=<%= project[:class_name] %> >
    <a href=<%= project[:link] %> target="_blank">
      <div class="title-box">
        <div class="headline">
          <h4><%= project[:name] %></h4>
          <p><%= project[:description] %></p><br />
          <p><%= project[:technology] %></p><br />
        </div>
        <div class="plus thin-plus">+</div>
      </div>
      <div class="overlay"></div>
    </a>
  </div>
</li>
